{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "dist",
    "rootDir": "Accordio",
    "noEmit": true
  },
  "include": [
    "Accordio/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "coverage",
    "Accordio/__tests__"
  ]
}